{
  "name": "u-client",
  "version": "9.0.0",
  "license": "MIT",
  "scripts": {
    "gql:update-schema": "graphql get-schema -e default",
    "ng": "ng",
    "start": "ng serve --open -c=dev1",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "buildProduction": "ng build --configuration production --base-href ./",
    "lint": "ng lint",
    "test": "ng lint && ng test --configuration=test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
    "e2e": "ng e2e",
    "ci": "npm run format:test && ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && npm run e2e && npm run build:prod -- --deploy-url /angular-ngrx-material-starter/ --base-href /angular-ngrx-material-starter",
    "format:write": "prettier src/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier src/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/angular-ngrx-material-starter/stats-es2015.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^8.11.2",
    "@angular-extensions/model": "^8.1.0",
    "@angular/animations": "^12.2.9",
    "@angular/cdk": "~12.2.9",
    "@angular/common": "~12.2.9",
    "@angular/compiler": "~12.2.9",
    "@angular/core": "~12.2.9",
    "@angular/forms": "~12.2.9",
    "@angular/http": "^8.0.0-beta.10",
    "@angular/localize": "^12.2.9",
    "@angular/material": "~12.2.9",
    "@angular/material-moment-adapter": "^12.2.9",
    "@angular/platform-browser": "~12.2.9",
    "@angular/platform-browser-dynamic": "~12.2.9",
    "@angular/router": "~12.2.9",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ng-idle/core": "^9.0.0-beta.1",
    "@ng-idle/keepalive": "^9.0.0-beta.1",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/file-saver": "^2.0.1",
    "bootstrap": "^4.5.0",
    "browser-detect": "^0.2.28",
    "chart.js": "^2.9.4",
    "classlist.js": "^1.1.20150312",
    "core-util-is": "^1.0.2",
    "file-saver": "^2.0.2",
    "ionicons": "^5.1.2",
    "moment": "^2.27.0",
    "ng2-charts": "^2.4.3",
    "rxjs": "^6.5.5",
    "tslib": "^2.3.1",
    "uuid": "^3.3.3",
    "web-animations-js": "^2.3.2",
    "webpack-dev-server": "^3.11.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "^12.2.9",
    "@angular/compiler-cli": "~12.2.9",
    "@angular/language-service": "~12.2.9",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@ngrx/schematics": "^8.6.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.9",
    "all-contributors-cli": "^6.16.1",
    "codelyzer": "^6.0.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "standard-version": "^9.3.1",
    "ts-node": "~8.5.4",
    "tslint": "~6.1.0",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
